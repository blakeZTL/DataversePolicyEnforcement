    "outDir": "dist",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noImplicitAny": true,
    "skipLibCheck": true
  },
  "include": [
    "src",
    "tests",
    ".eslintrc.js",
    "."
